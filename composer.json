{
    "name": "indigoram89/laravel-fetchable",
    "description": "Fetchable models for Laravel 5",
    "keywords": [
        "indigoram89", "laravel", "fetchable", "model"
    ],
    "homepage": "https://github.com/indigoram89/laravel-fetchable",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Orlov",
            "email": "maksimorlov.personal@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": ">=5.3",
        "illuminate/support": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "orchestra/testbench": "^3.4.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Indigoram89\\Fetchable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indigoram89\\Fetchable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
